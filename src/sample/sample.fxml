<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.canvas.*?>
<?import javafx.scene.*?>
<?import javafx.scene.chart.*?>
<?import javafx.scene.effect.*?>
<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<FlowPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller">
   <children>
      <HBox fx:id="titleView" alignment="CENTER_LEFT" minHeight="-Infinity" minWidth="-Infinity" prefHeight="58.0" prefWidth="602.0">
         <children>
            <Label fx:id="titleLabel" alignment="CENTER" contentDisplay="BOTTOM" prefHeight="57.0" prefWidth="172.0" text="Title">
               <HBox.margin>
                  <Insets left="50.0" />
               </HBox.margin>
               <font>
                  <Font size="24.0" />
               </font>
            </Label>
         </children>
      </HBox>
      <VBox prefHeight="277.0" prefWidth="602.0">
         <children>
            <HBox alignment="CENTER_LEFT" prefHeight="35.0" prefWidth="602.0">
               <children>
                  <Label fx:id="option1Label" text="option1">
                     <HBox.margin>
                        <Insets right="20.0" />
                     </HBox.margin>
                     <padding>
                        <Insets left="20.0" />
                     </padding>
                     <font>
                        <Font size="18.0" />
                     </font>
                     <effect>
                        <Blend />
                     </effect>
                  </Label>
                  <ComboBox fx:id="option1Combo" />
               </children>
               <VBox.margin>
                  <Insets bottom="30.0" top="50.0" />
               </VBox.margin>
            </HBox>
            <HBox alignment="CENTER_LEFT" prefHeight="35.0" prefWidth="602.0">
               <children>
                  <Label fx:id="option2Label" text="option2">
                     <HBox.margin>
                        <Insets right="20.0" />
                     </HBox.margin>
                     <padding>
                        <Insets left="20.0" />
                     </padding>
                     <font>
                        <Font size="18.0" />
                     </font>
                     <effect>
                        <Blend />
                     </effect>
                  </Label>
                  <ComboBox fx:id="option2Combo" />
               </children>
            </HBox>
         </children>
      </VBox>
      <HBox alignment="CENTER_RIGHT" minHeight="-Infinity" prefHeight="62.0" prefWidth="602.0">
         <children>
            <Button fx:id="nextButton" mnemonicParsing="false" onAction="#nextOption" prefHeight="43.0" prefWidth="91.0" text="Next">
               <HBox.margin>
                  <Insets right="50.0" />
               </HBox.margin>
            </Button>
         </children>
      </HBox>
   </children>
</FlowPane>
